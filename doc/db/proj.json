    { "0": [
        { "line": 300,
          "s_id": "678e469a280e7220ca209c3c",
          "text": "姚秦天竺三藏鳩摩羅什"}
      ],
      "1": [
        { "line": 300,
          "s_id": "678e46d7280e7220ca209c3e",
          "text": "唐三藏法師玄奘譯"}
      ],
      "2": [
        { "line": 102,
          "s_id": "679df7076c12443daf2882ad",
          "text": "摩竭提國三藏沙門法月"}
      ],
      "3": [
        { "line": 300,
          "s_id": "67adc962d41ae868db888aa6",
          "text": "國大德三藏法師沙門法"}
