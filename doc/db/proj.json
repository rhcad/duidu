    { "0": [],
      "1": [],
      "2": [
        { "line": 200,
          "s_id": "679df7076c12443daf2882ad",
          "text": "如是我聞："},
        { "line": 300,
          "s_id": "679df7076c12443daf2882ad",
          "text": "一時佛在王舍大城靈鷲"},
        { "line": 400,
          "s_id": "679df7076c12443daf2882ad",
          "text": "爾時觀自在菩薩摩訶薩"}
      ],
      "3": [
        { "line": 400,
          "s_id": "67adc962d41ae868db888aa6",
          "text": "如是我聞："},
        { "line": 500,
          "s_id": "67adc962d41ae868db888aa6",
          "text": "一時薄伽梵住王舍城鷲"},
        { "line": 600,
          "s_id": "67adc962d41ae868db888aa6",
          "text": "時，具壽舍利子，承佛"}
      ],
      "row_i": 4
    },
    { "0": [
        { "line": 400,
          "s_id": "678e469a280e7220ca209c3c",
          "text": "觀世音菩薩行深般若波"}
      ],
      "1": [
        { "line": 400,
          "s_id": "678e46d7280e7220ca209c3e",
          "text": "觀自在菩薩行深般若波"}
      ],
      "2": [
        { "line": 500,
          "s_id": "679df7076c12443daf2882ad",
          "text": "於是觀自在菩薩摩訶薩"}
      ],
      "3": [
        { "line": 700,
          "s_id": "67adc962d41ae868db888aa6",
          "text": "觀自在菩薩摩訶薩答具"}
