    { "0": [
        { "line": 1400,
          "s_id": "67adfa99fb55bd1a42b7cfa0",
          "text": "「以無所得故，菩薩依"}
      ],
      "1": [
        { "line": 1300,
          "s_id": "67adfa99fb55bd1a42b7cfa1",
          "text": "「以無所得故，菩提薩"}
      ],
      "2": [
        { "line": 607,
          "s_id": "67adfa99fb55bd1a42b7cfa4",
          "text": "以無所得故，菩提薩埵"},
        { "line": 608,
          "s_id": "67adfa99fb55bd1a42b7cfa4",
          "text": "無罣礙故，無有恐怖，"}
      ],
      "3": [
        { "line": 708,
          "s_id": "67adfa99fb55bd1a42b7cfa5",
          "text": "是故舍利子！以無所得"}
      ],
      "row_i": 12
    },
    { "0": [
        { "line": 1500,
          "s_id": "67adfa99fb55bd1a42b7cfa0",
          "text": "三世諸佛依般若波羅蜜"}
      ],
      "1": [
        { "line": 1400,
          "s_id": "67adfa99fb55bd1a42b7cfa1",
          "text": "三世諸佛依般若波羅蜜"}
      ],
      "2": [
        { "line": 609,
          "s_id": "67adfa99fb55bd1a42b7cfa4",
          "text": "三世諸佛依般若波羅蜜"}
      ],
      "3": [
        { "line": 709,
          "s_id": "67adfa99fb55bd1a42b7cfa5",
          "text": "三世一切諸佛亦皆依般"}
      ],
      "row_i": 13
    },
    { "0": [
        { "line": 1600,
          "s_id": "67adfa99fb55bd1a42b7cfa0",
          "text": "「故知般若波羅蜜是大"}
      ],
      "1": [
        { "line": 1500,
          "s_id": "67adfa99fb55bd1a42b7cfa1",
          "text": "「故知般若波羅蜜多是"}
      ],
      "2": [
        { "line": 610,
          "s_id": "67adfa99fb55bd1a42b7cfa4",
          "text": "故知般若波羅蜜多是大"}
      ],
      "3": [
        { "line": 710,
          "s_id": "67adfa99fb55bd1a42b7cfa5",
          "text": "是故當知般若波羅蜜多"}
      ],
      "row_i": 14
    },
    { "0": [
        { "line": 1700,
          "s_id": "67adfa99fb55bd1a42b7cfa0",
          "text": "即說呪曰："},
        { "line": 1800,
          "s_id": "67adfa99fb55bd1a42b7cfa0",
          "text": "「竭帝　竭帝　波羅竭"}
      ],
      "1": [
        { "line": 1600,
          "s_id": "67adfa99fb55bd1a42b7cfa1",
          "text": "即說咒曰："},
        { "line": 1700,
          "s_id": "67adfa99fb55bd1a42b7cfa1",
          "text": "「揭帝　揭帝　般羅揭"}
      ],
      "2": [
        { "line": 700,
          "s_id": "67adfa99fb55bd1a42b7cfa4",
          "text": "即說呪曰："},
        { "line": 800,
          "s_id": "67adfa99fb55bd1a42b7cfa4",
          "text": "「揭諦　揭諦　波羅揭"}
      ],
      "3": [
        { "line": 900,
          "s_id": "67adfa99fb55bd1a42b7cfa5",
          "text": "「峩帝　峩帝　波囉峩"},
        { "line": 800,
          "s_id": "67adfa99fb55bd1a42b7cfa5",
          "text": "即說般若波羅蜜多咒曰"}
      ],
      "row_i": 15
    },
